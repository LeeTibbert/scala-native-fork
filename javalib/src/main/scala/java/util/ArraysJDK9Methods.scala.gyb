  /** @since JDK 9 */
  def compare(a: Array[scala.${T}], b: Array[scala.${T}]): Int =
    compareImpl(a, b, jl.${javaType}.compare)

  /** @since JDK 9 */
  def compare(
      a: Array[scala.${T}],
      aFromIndex: Int,
      aToIndex: Int,
      b: Array[scala.${T}],
      bFromIndex: Int,
      bToIndex: Int
  ): Int =
    compareImpl(
      a,
      aFromIndex,
      aToIndex,
      b,
      bFromIndex,
      bToIndex,
      jl.${javaType}.compare
    )

